<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.mypage.MyPageFragment">

    <ImageView
        android:id="@+id/iv_profile_mypage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="138dp"
        android:src="@drawable/img_profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_name_mypage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="142dp"
        android:layout_marginTop="17dp"
        android:fontFamily="@font/noto_sans_kr_500"
        android:includeFontPadding="false"
        android:text="강규태"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_profile_mypage" />

    <ImageView
        android:id="@+id/btn_edit_mypage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="202dp"
        android:layout_marginTop="265dp"
        android:src="@drawable/ic_edit_mypage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_mypage"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginTop="48dp"
        android:background="#F7F8FA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_name_mypage" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="366dp"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_iconfeed_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="22dp"
                android:layout_marginTop="33dp"
                android:src="@drawable/ic_feed_mypage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_feed_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="33dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="운동 피드"
                android:textColor="@color/black"
                android:textSize="16dp"
                app:layout_constraintStart_toEndOf="@+id/iv_iconfeed_mypage"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="40dp"
                android:src="@drawable/ic_btn_mypage"
                app:layout_constraintStart_toEndOf="@+id/tv_feed_mypage"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_time_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="72dp"
                android:layout_marginTop="35dp"
                android:fontFamily="@font/noto_sans_kr_500"
                android:includeFontPadding="false"
                android:text="16:03"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_feed_mypage" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="90dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="시간"
                android:textColor="#818594"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_time_mypage" />

            <TextView
                android:id="@+id/tv_distance_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="237dp"
                android:layout_marginTop="35dp"
                android:fontFamily="@font/noto_sans_kr_500"
                android:includeFontPadding="false"
                android:text="10.45km"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_feed_mypage" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="262dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="거리"
                android:textColor="#818594"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_distance_mypage" />

            <TextView
                android:id="@+id/tv_face_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="68dp"
                android:layout_marginTop="46dp"
                android:fontFamily="@font/noto_sans_kr_500"
                android:includeFontPadding="false"
                android:text="3km/h"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_time_mypage" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="85dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="페이스"
                android:textColor="#818594"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_face_mypage" />

            <TextView
                android:id="@+id/tv_kcal_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="242dp"
                android:layout_marginTop="46dp"
                android:fontFamily="@font/noto_sans_kr_500"
                android:includeFontPadding="false"
                android:text="471kcal"
                android:textColor="@color/black"
                android:textSize="20dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_distance_mypage" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="262dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="칼로리"
                android:textColor="#818594"
                android:textSize="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_kcal_mypage" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="47dp"
                android:layout_marginEnd="21dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="2022년 11월 7일 17:50"
                android:textColor="#818594"
                android:textSize="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_kcal_mypage" />

            <ImageView
                android:id="@+id/iv_badge_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="22dp"
                android:layout_marginTop="226dp"
                android:src="@drawable/ic_badge_mypage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_iconfeed_mypage" />

            <TextView
                android:id="@+id/tv_badge_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="219dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="수집한 배지"
                android:textColor="@color/black"
                android:textSize="16dp"
                app:layout_constraintStart_toEndOf="@+id/iv_badge_mypage"
                app:layout_constraintTop_toBottomOf="@+id/tv_feed_mypage"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="226dp"
                android:src="@drawable/ic_btn_mypage"
                app:layout_constraintStart_toEndOf="@+id/tv_badge_mypage"
                app:layout_constraintTop_toBottomOf="@id/tv_feed_mypage" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="43dp"
                android:layout_marginTop="34dp"
                android:src="@drawable/badge1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_badge_mypage" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="123dp"
                android:layout_marginTop="30dp"
                android:src="@drawable/badge2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_badge_mypage" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="202dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/badge3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_badge_mypage" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="273dp"
                android:layout_marginTop="32dp"
                android:src="@drawable/badge4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_badge_mypage" />

            <ImageView
                android:id="@+id/iv_friend_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="22dp"
                android:layout_marginTop="166dp"
                android:src="@drawable/ic_friend_mypage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_badge_mypage" />

            <TextView
                android:id="@+id/tv_friend_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="161dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="친구 관리"
                android:textColor="@color/black"
                android:textSize="16dp"
                app:layout_constraintStart_toEndOf="@+id/iv_friend_mypage"
                app:layout_constraintTop_toBottomOf="@id/tv_badge_mypage" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="168dp"
                android:src="@drawable/ic_btn_mypage"
                app:layout_constraintStart_toEndOf="@+id/tv_friend_mypage"
                app:layout_constraintTop_toBottomOf="@id/tv_badge_mypage" />

            <ImageView
                android:id="@+id/iv_addfriend_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="22dp"
                android:layout_marginTop="48dp"
                android:src="@drawable/ic_addfriend_mypage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_friend_mypage" />

            <ImageView
                android:id="@+id/iv_addfriendpkus_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="34dp"
                android:layout_marginTop="43dp"
                android:src="@drawable/ic_addfriend_plus_mypage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_friend_mypage" />

            <TextView
                android:id="@+id/tv_addfriend_mypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="36dp"
                android:fontFamily="@font/noto_sans_kr_400"
                android:includeFontPadding="false"
                android:text="친구 초대링크"
                android:textColor="@color/black"
                android:textSize="16dp"
                app:layout_constraintStart_toEndOf="@+id/iv_addfriend_mypage"
                app:layout_constraintTop_toBottomOf="@id/tv_friend_mypage" />


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="43dp"
                android:src="@drawable/ic_btn_mypage"
                app:layout_constraintStart_toEndOf="@+id/tv_addfriend_mypage"
                app:layout_constraintTop_toBottomOf="@id/tv_friend_mypage" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>