            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/info_site_menu"
            android:layout_marginStart="10dp"
            android:layout_marginTop="696dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_bottom_fourth_menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/info_setting_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="환경설정"
            android:fontFamily="@font/noto_sans_kr_400"
            android:includeFontPadding="false"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="16dp"
            android:textColor="#1E2026"
            android:layout_marginStart="233dp"
            android:layout_marginTop="685dp"/>

        <ImageView
            android:id="@+id/info_settingbtn_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_bottombtn_menu"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/info_setting_menu"
            android:layout_marginStart="10dp"
            android:layout_marginTop="693dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>